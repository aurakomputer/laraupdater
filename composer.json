{
    "name": "aurakomputer/laraupdater",
    "description": "LaraUpdater allows your Laravel application to auto-update itself.",
    "keywords": [
        "self-updater",
        "update-checker",
        "update-installer",
        "laravel",
        "backup-restore"
    ],
    "homepage": "https://github.com/aurakomputer/laraupdater",
    "require": {
        "php": ">=7.0.0 || ^8.0",
        "illuminate/support": "^8.28 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pietro Cinaglia",
            "homepage": "https://github.com/pietrocinaglia",
            "role": "Software Engineer."
        },
        {
            "name": "Mohamad Supangat",
            "homepage": "https://github.com/mohamad-supangat",
            "role": "Software Engineer."
        },
        {
            "name": "AuraKomputer",
            "homepage": "https://github.com/aurakomputer",
            "role": "Software House."
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "pcinaglia\\laraupdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pcinaglia\\laraupdater\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pcinaglia\\laraupdater\\LaraUpdaterServiceProvider"
            ]
        }
    }
}
